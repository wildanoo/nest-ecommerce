{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ecommerce-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/ecommerce-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/ecommerce-api",
  "projects": {
    "ecommerce-api": {
      "type": "application",
      "root": "apps/ecommerce-api",
      "entryFile": "main",
      "sourceRoot": "apps/ecommerce-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ecommerce-api/tsconfig.app.json"
      }
    },
    "product": {
      "type": "application",
      "root": "apps/product",
      "entryFile": "main",
      "sourceRoot": "apps/product/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product/tsconfig.app.json"
      }
    },
    "user": {
